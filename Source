        SitinkNotify.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

        Notification.Name = "Notification"
        Notification.Parent = SitinkNotify
        Notification.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        Notification.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Notification.BorderSizePixel = 0
        Notification.Position = UDim2.new(2.745269299, 0, 0.850202441, 0)
        Notification.Size = UDim2.new(0, 268, 0, 63)

        UICorner.CornerRadius = UDim.new(0, 4)
        UICorner.Parent = Notification

        UIStroke.Color = Color3.fromRGB(255, 133, 135)
        UIStroke.Thickness = 1.5
        UIStroke.Parent = Notification

        Features.Name = "Features"
        Features.Parent = Notification
        Features.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
