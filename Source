        UIPadding.Parent = Title
        UIPadding.PaddingLeft = UDim.new(0, 4)

        Desc.Name = "Desc"
        Desc.Parent = Features
        Desc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Desc.BackgroundTransparency = 1.000
        Desc.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Desc.BorderSizePixel = 0
        Desc.Position = UDim2.new(0, 0, 0, 23)
        Desc.Size = UDim2.new(1, 0, 0.0634920672, 35)
        Desc.Font = Enum.Font.GothamBold
        Desc.Text = Configs.Content
        Desc.TextColor3 = Color3.fromRGB(70, 70, 70)
        Desc.TextSize = 12.000
        Desc.TextXAlignment = Enum.TextXAlignment.Left
        Desc.TextYAlignment = Enum.TextYAlignment.Top

        UIPadding_2.Parent = Desc
        UIPadding_2.PaddingLeft = UDim.new(0, 4)

        Close.Name = "Close"
        Close.Parent = Features
        Close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Close.BackgroundTransparency = 1.000
        Close.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Close.BorderSizePixel = 0
        Close.Position = UDim2.new(1, -20, 0, 0)
        Close.Size = UDim2.new(0, 20, 0, 20)

        ImageLabel.Parent = Close
        ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        ImageLabel.BackgroundTransparency = 1.000
        ImageLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
        ImageLabel.BorderSizePixel = 0
        ImageLabel.Position = UDim2.new(0.25, 0, 0.25, 0)
        ImageLabel.Size = UDim2.new(0, 10, 0, 10)
        ImageLabel.Image = "rbxassetid://18402638045"

        click.Name = "click"
        click.Parent = Close
        click.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        click.BackgroundTransparency = 1.000
        click.BorderColor3 = Color3.fromRGB(0, 0, 0)
        click.BorderSizePixel = 0
        click.Size = UDim2.new(1, 0, 1, 0)
        click.Font = Enum.Font.SourceSans
        click.Text = ""
        click.TextColor3 = Color3.fromRGB(0, 0, 0)
        click.TextSize = 14.000

        click.MouseButton1Click:Connect(function()
            CircleClick(click, Mouse.X, Mouse.Y)
            TweenService:Create(Notification, TweenInfo.new(3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Position = UDim2.new(2.745269299, 0, 0.850202441, 0)}):Play() 
            task.wait(tonumber(Configs.Time) / 2)
            Notification.Visible = false
            task.wait(tonumber(Configs.Time) / 2)
            Notification:Destroy()
        end)

        TweenService:Create(Notification, TweenInfo.new(3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Position = UDim2.new(0.745269299, 0, 0.850202441, 0)}):Play()
        task.wait(tonumber(Configs.Time))
        TweenService:Create(Notification, TweenInfo.new(3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Position = UDim2.new(2.745269299, 0, 0.850202441, 0)}):Play() 
        task.wait(tonumber(Configs.Time) / 2)
        Notification.Visible = false
        task.wait(tonumber(Configs.Time) / 2)
        Notification:Destroy()
    end)
end

