                    TweenService:Create(Title_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = Color3.fromRGB(70, 70, 70)}):Play()
                    TweenService:Create(CheckerFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1.000}):Play()
                    TweenService:Create(CheckerFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(70, 70, 70)}):Play()
                    TweenService:Create(UIStroke_4, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(70, 70, 70)}):Play()
                    TweenService:Create(Frame_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0, 2, 0, 1)}):Play()
                    TweenService:Create(Frame_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(70, 70, 70)}):Play()
                end
                HasToggle = not HasToggle
                pcall(Configs.Callback, HasToggle)
            end)

            if Configs.Default == true then
                TweenService:Create(Title_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = Color3.fromRGB(10, 157, 255)}):Play()
                TweenService:Create(CheckerFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0.000}):Play()
                TweenService:Create(CheckerFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(10, 157, 255)}):Play()
                TweenService:Create(UIStroke_4, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(10, 157, 255)}):Play()
                TweenService:Create(Frame_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0, 23, 0, 1)}):Play()
                TweenService:Create(Frame_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                HasToggle = true
                pcall(Configs.Callback, HasToggle)
            end

            function FuncToggle:Set(Value)
                if Value == true then
                    TweenService:Create(Title_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = Color3.fromRGB(10, 157, 255)}):Play()
                    TweenService:Create(CheckerFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0.000}):Play()
                    TweenService:Create(CheckerFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(10, 157, 255)}):Play()
                    TweenService:Create(UIStroke_4, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(10, 157, 255)}):Play()
                    TweenService:Create(Frame_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0, 23, 0, 1)}):Play()
