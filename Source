            Cat.Parent = List
            Cat.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Cat.BackgroundTransparency = 1.000
            Cat.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Cat.BorderSizePixel = 0
            Cat.Selectable = false
            Cat.Size = UDim2.new(1, 0, 1, 0)
            Cat.ScrollBarThickness = 1

            UIListLayout_2.Parent = Cat
            UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
            UIListLayout_2.Padding = UDim.new(0, 6)

            UIPadding_9.Parent = Cat
            UIPadding_9.PaddingTop = UDim.new(0, 4)

            Configs.Callback(Configs.Default)

            for i, v in next, Configs.List do
                local Option_2 = Instance.new("TextButton")
                local UIPadding_11 = Instance.new("UIPadding")

                Option_2.Name = "Option"
                Option_2.Parent = Cat
                Option_2.Active = false
                Option_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Option_2.BackgroundTransparency = 1.000
                Option_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Option_2.BorderSizePixel = 0
                Option_2.Selectable = false
                Option_2.Size = UDim2.new(1, 0, 0, 20)
                Option_2.Font = Enum.Font.GothamBold
                Option_2.Text = tostring(v)
                Option_2.TextColor3 = Color3.fromRGB(70, 70, 70)
                Option_2.TextSize = 14.000
                Option_2.TextXAlignment = Enum.TextXAlignment.Left

                UIPadding_11.Parent = Option_2
                UIPadding_11.PaddingLeft = UDim.new(0, 7)

                Option_2.MouseButton1Click:Connect(function()
                    CircleClick(Option_2, Mouse.X, Mouse.Y)
                    TweenService:Create(ImageLabel_4, TweenInfo.new(.3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Rotation = -90.000}):Play() 
                    Dropdown:TweenSize(UDim2.new(1, 0, -0, 55),"Out","Quad",0.3,true)
                    Title_3.Text = Configs.Name .. " : " .. Option_2.Text
                    Configs.Callback(Option_2.Text)
                end)
            end

            Click_4.MouseButton1Click:Connect(function()
                CircleClick(Click_4, Mouse.X, Mouse.Y)
                if Dropped == false then
                    Dropped = true
                    TweenService:Create(ImageLabel_4, TweenInfo.new(.3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Rotation = 0}):Play() 
                    Dropdown:TweenSize(UDim2.new(1, 0, 0.131, 55),"Out","Quad",0.3,true)
                else
                    Dropped = false
                    TweenService:Create(ImageLabel_4, TweenInfo.new(.3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Rotation = -90.000}):Play() 
                    Dropdown:TweenSize(UDim2.new(1, 0, 0, 55),"Out","Quad",0.3,true)
                end
            end)

            function DropFunc:Set(a)
                for i, v in pairs(Cat:GetChildren()) do
                    if v.ClassName == "TextButton" then
                        if v.Name == a then
                            v.TextColor3 = Color3.fromRGB(255, 255, 255)
                            Title_3.Text = Configs.Name .. " : " .. OptionName
                            Configs.Callback(Value_2.Text)
                        end
                    end
                end
            end
            function DropFunc:Add(Option)
                for i, v in next, Option do
                    local Nigga = Instance.new("TextButton")
                    local UIPadding_11 = Instance.new("UIPadding")

                    Nigga.Name = "Option"
                    Nigga.Parent = Cat
                    Nigga.Active = false
                    Nigga.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    Nigga.BackgroundTransparency = 1.000
                    Nigga.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Nigga.BorderSizePixel = 0
                    Nigga.Selectable = false
                    Nigga.Size = UDim2.new(1, 0, 0, 20)
                    Nigga.Font = Enum.Font.GothamBold
                    Nigga.Text = tostring(v)
                    Nigga.TextColor3 = Color3.fromRGB(70, 70, 70)
                    Nigga.TextSize = 14.000
                    Nigga.TextXAlignment = Enum.TextXAlignment.Left

                    UIPadding_11.Parent = Nigga
                    UIPadding_11.PaddingLeft = UDim.new(0, 7)

                    Nigga.MouseButton1Click:Connect(function()
                        CircleClick(Nigga, Mouse.X, Mouse.Y)
                        TweenService:Create(ImageLabel_4, TweenInfo.new(.3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Rotation = -90.000}):Play() 
                        Dropdown:TweenSize(UDim2.new(1, 0, -0, 55),"Out","Quad",0.3,true)
                        Title_3.Text = Configs.Name .. " : " .. Nigga.Text
                        Configs.Callback(Nigga.Text)
                    end)
                end
            end
            function DropFunc:Refresh(a)
                for i, v in pairs(Cat:GetChildren()) do
                    if v.ClassName == "TextButton" then
                        v:Destroy()
                    end
                end
                Dropped = false
                TweenService:Create(ImageLabel_4, TweenInfo.new(.3, Enum.EasingStyle.Back, Enum.EasingDirection.InOut),{Rotation = -90.000}):Play()
                DropFunc:Add(a)
                Title_3.Text = Configs.Name 
            end
            return DropFunc
        end
        function FuncInTab:MakeSlider(Configs)
            Configs = Configs or {}
            Configs.Name = Configs.Name
            Configs.Max = Configs.Max or 100
            Configs.Min = Configs.Min or 50
            Configs.Default = Configs.Default or 20
            Configs.Callback = Configs.Callback or function() end

            local Slider = Instance.new("Frame")
            local UICorner_12 = Instance.new("UICorner")
            local Features_4 = Instance.new("Frame")
            local Title_4 = Instance.new("TextLabel")
