        function FuncInTab:MakeDropdown(Configs)
            Configs.Name = Configs.Name or "Teru nigga"
            Configs = Configs or {}
            Configs.Description = Configs.Description or ""
            Configs.List = Configs.List or {"1", "2"}
            Configs.Callback = Configs.Callback or function() end
            Configs.Default = Configs.Default or ""

            local Dropdown = Instance.new("Frame")
            local UICorner_11 = Instance.new("UICorner")
            local Features_3 = Instance.new("Frame")
            local Title_3 = Instance.new("TextLabel")
            local UIPadding_7 = Instance.new("UIPadding")
            local Desc_3 = Instance.new("TextLabel")
            local UIPadding_8 = Instance.new("UIPadding")
            local ImageLabel_4 = Instance.new("ImageLabel")
            local Click_4 = Instance.new("TextButton")
            local List = Instance.new("Frame")
