        ClickTab.MouseButton1Click:Connect(function()
            CircleClick(ClickTab, Mouse.X, Mouse.Y)
            for i, v in pairs(ChannelContainer:GetChildren()) do
                if v.Name == "Channel" then
                    v.Visible = false
                end
                Channel.Visible = true
            end
            for i, v in pairs(Tab:GetChildren()) do
                if v:IsA("Frame") then
                    v.BackgroundTransparency = 1.000
                end
            end
            for i, v in pairs(Tab.Tab2:GetChildren()) do
                if v:IsA("TextLabel") then
                    v.TextColor3 = Color3.fromRGB(70, 70, 70)
                end
            end
            for i, v in pairs(Tab.Tab2:GetChildren()) do
                if v.ClassName == "ImageLabel" then
                    v.ImageColor3 = Color3.fromRGB(70, 70, 70)
                end
            end
            NameTab_2.TextColor3 = Color3.fromRGB(255, 255, 255)
            ImageLabel_7.ImageColor3 = Color3.fromRGB(255, 255, 255)
            Tab2.BackgroundTransparency = 0.930
        end)

        if default == false then
            Channel.Visible = false
            ChannelContainer.Visible = true
            Tab2.BackgroundTransparency = 1.000
            NameTab_2.TextColor3 = Color3.fromRGB(70, 70, 70)
        end
